{% extends 'base.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
<h2 class="text-center mt-5 mb-3">Show Project</h2>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ project.Name }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ project.Age }}</td>
            </tr>
            <tr>
                <th>Gender</th>
                <td>{{ project.Gender }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ project.City }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-success" href="{{ path('app_project_index') }}">back to list</a>

    <a class="btn btn-outline-success" href="{{ path('app_project_edit', {'id': project.id}) }}">edit</a>

    {{ include('project/_delete_form.html.twig') }}
{% endblock %}
